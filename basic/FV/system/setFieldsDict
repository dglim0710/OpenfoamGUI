FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue h 0
);

regions
(
    cylinderToCell
    {
        P1 (0 0 0);
        P2 (0 0 1);
        fieldValues
        (
            volScalarFieldValue alpha1 0.005
        );
    }
);


